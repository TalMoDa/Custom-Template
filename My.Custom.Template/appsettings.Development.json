{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [],
    "WriteTo": [{ "Name": "Console" }]
  },
  
  "ConnectionStrings": {
    "DefaultConnection": "{SQL_CONNECTION_STRING}"
  }
}
